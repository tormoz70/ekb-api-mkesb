<?xml version="1.0" encoding="UTF-8"?>
<cursor xmlns="http://bio4j.ru/biodef-repo/store"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://bio4j.ru/biodef-repo/store http://bio4j.ru/biodef-repo/store/store-v11.xsd"
        multiselection="true">
    <SQL action="select">
        <text><![CDATA[
select * from table(${ktpub_api.current.vesion}.getComps(:p_released, :p_subn, :p_query))
        ]]></text>
        <param type="string" name="p_released"/>
        <param type="string" name="p_subn"/>
        <param type="string" name="p_query"/>
    </SQL>
    <fields>
        <field type="string" name="ogrn" pk="true"/>
        <field type="string" name="studia"/>
        <field type="string" name="pcodes"/>
        <field type="string" name="filmnames"/>
        <field type="string" name="flags"/>
        <field type="string" name="comps_count"/>
        <field type="string" name="films"/>
        <field type="string" name="govsupp_rev"/>
        <field type="string" name="govsupp_irr"/>
        <field type="string" name="summ"/>
        <field type="string" name="tckts"/>
    </fields>
</cursor>