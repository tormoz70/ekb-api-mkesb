<?xml version="1.0" encoding="UTF-8"?>
<cursor xmlns="http://bio4j.ru/biodef-repo/store"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://bio4j.ru/biodef-repo/store http://bio4j.ru/biodef-repo/store/store-v11.xsd"
        multiselection="true">
    <SQL action="select">
        <text><![CDATA[
select * from table(${ktpub_api.current.vesion}.getAwardsInprod(:p_subn, :p_search_query))
        ]]></text>
        <param type="string" name="p_subn"/>
        <param type="string" name="p_search_query"/>
    </SQL>
    <fields>
        <field type="integer" name="faward_id" pk="true"/>
        <field type="string" name="pcode"/>
        <field type="string" name="fest_uid"/>
        <field type="string" name="fest_name"/>
        <field type="string" name="icon"/>
        <field type="boolean" name="isnomination"/>
        <field type="string" name="award_aname"/>
        <field type="string" name="awardee"/>
    </fields>
</cursor>